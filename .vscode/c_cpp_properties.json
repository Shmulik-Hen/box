{
	"configurations": [
		{
			"name": "Linux",
			"includePath": [
				"${workspaceFolder}/inc/*"
			],
			"defines": [ ],
			"cStandard": "c23",
			"cppStandard": "gnu++23",
			"intelliSenseMode": "linux-gcc-x64",
			"compilerPath": "/usr/bin/g++",
			"configurationProvider": "ms-code.makefile-tools",
			"browse": {
				"path": [
					"/usr/include"
				],
				"limitSymbolsToIncludedHeaders": true
			}
		}
	],
	"version": 4
}